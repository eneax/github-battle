{
  "name": "github-battle",
  "version": "1.0.0",
  "description": "React app built using React, Webpack, Babel, and Netlify.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV='production' webpack",
    "build-for-windows": "set NODE_ENV=production&&webpack"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "syntax-dynamic-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eneax/github-battle.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "netlify"
  ],
  "author": "Enea Xharja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eneax/github-battle/issues"
  },
  "homepage": "https://github.com/eneax/github-battle#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^6.3.2",
    "prop-types": "^15.8.1",
    "query-string": "^6.13.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
